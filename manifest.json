{
    "name": "UIC Phishing Study",
    "version": "0.5",
    "manifest_version": 2,
    "description": "A tool used for measuring factors on whether a user is suceptable to phishing ",
    "options_page": "config.html",
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": [
                "js/UIC.js",
                "js/UIC/platforms/chrome/events.js",
                "js/UIC/pages/content.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "js/contrib/jquery-2.0.3.min.js",
            "js/contrib/sjcl.js",
            "js/contrib/parse_uri.js",
            "js/UIC.js",
            "js/UIC/constants.js",
            "js/UIC/platforms/chrome/events.js",
            "js/UIC/platforms/chrome/cookies.js",
            "js/UIC/platforms/chrome/storage.js",
            "js/UIC/models/webservices.js",
            "js/UIC/models/tabs.js",
            "js/UIC/models/user.js",
            "js/UIC/models/reauths.js",
            "js/UIC/models/rules.js",
            "js/UIC/pages/background.js"
        ]
    },
    "permissions": [
        "webNavigation",
        "cookies",
        "storage",
        "http://drano-dev.uicbits.net/*",
        "*://*/*"
    ]
}
